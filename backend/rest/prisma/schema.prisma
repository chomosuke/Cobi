generator client {
  provider = "prisma-client-js"
  output   = "./"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id                Int     @id @default(autoincrement())
  username          String  @unique @db.VarChar(128)
  password          String  @db.VarChar(512)
  profilePictureUrl String? @db.VarChar(4096)
}
